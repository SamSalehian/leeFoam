FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.2 0 0);

boundaryField
{
    empty
    {
        type            empty;
    }
    sides
    {
        type            zeroGradient;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform (0.2 0 0);
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform (0.2 0 0);
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform (0.2 0 0);
    }
}


